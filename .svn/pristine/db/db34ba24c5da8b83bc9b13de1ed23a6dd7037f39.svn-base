CREATE TABLE `medical` (
`CommentID` varchar(255) NOT NULL default '',
                          `UserID` varchar(255) default NULL,
                          `Examiner` varchar(255) default NULL,
                          `Testdate` timestamp NOT NULL,
                          `Data_entry_completion_status` enum('Incomplete','Complete') NOT NULL default 'Incomplete',
`Date_taken` date default NULL,
`Candidate_Age` varchar(255) default NULL,
`Window_Difference` varchar(255) default NULL,
`meds` text default NULL,
`meds_status` enum('not_answered') default NULL,
`ldopa` varchar(255) default NULL,
`ldopa_status` enum('not_answered') default NULL,
`dopa_a` varchar(255) default NULL,
`dopa_a_status` enum('not_answered') default NULL,
`anti_dep` varchar(255) default NULL,
`anti_dep_status` enum('not_answered') default NULL,
`chol` varchar(255) default NULL,
`chol_status` enum('not_answered') default NULL,
`av_off_per24hr` numeric default NULL,
`av_off_per24hr_status` enum('not_answered') default NULL,
`av_awake_per24` numeric default NULL,
`av_awake_per24_status` enum('not_answered') default NULL,
`w_dys` numeric default NULL,
`w_dys_status` enum('not_answered') default NULL,
`w_t_dys` numeric default NULL,
`w_t_dys_status` enum('not_answered') default NULL,
`pct_off` numeric default NULL,
`pct_off_status` enum('not_answered') default NULL,
`pct_dys` numeric default NULL,
`pct_dys_status` enum('not_answered') default NULL,
PRIMARY KEY  (`CommentID`)
);
REPLACE INTO test_names (Test_name, Full_name, Sub_group) VALUES ('medical', 'Medical Summary', 1);
